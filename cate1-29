def sum_of_squares(N):
    return sum(i**2 for i in range(1, N + 1))

def square_of_sum(N):
    return sum(range(1, N + 1))**2

def difference_of_sum_of_square_and_square_of_sum(N):
    return square_of_sum(N) - sum_of_squares(N)

# Example usage
N = int(input("Enter the value of N: "))
difference = difference_of_sum_of_square_and_square_of_sum(N)
print("Difference between sum of squares and square of sum for the first", N, "natural numbers is:", difference)
